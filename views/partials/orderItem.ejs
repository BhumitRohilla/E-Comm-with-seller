<div class="admin-item-container">
    <img class="item-img" src=<%=`/image/product/${item.img}`%> alt="">
    <div class="table">
      <div class="tr">
        <div class="td">Title</div>
        <div class="td"><%=item.title%></div>
      </div>
      <div class="tr">
        <div class="td">Price</div>
        <div class="td">
          <% if (item.price == 0){ %>
            <%='FREE'%>
          <%}else{%>
            <%='â‚¹ '+item.price%>
          <%}%></div>
      </div>
      <div class="tr">
        <div class="td">Tag</div>
        <div class="td"><%=item.tag.join(' ')%></div>
      </div>
      <div class="tr">
        <div class="td">Date</div>
        <div class="td"><%=item.date%></div>
      </div>
      <div class="tr">
        <div class="td">Status</div>
        <div class="td"><%=item.status%></div>
      </div>
      <div class="tr">
        <div class="td">UserReviews</div>
        <div class="td"><%=item.userReviews%></div>
      </div>
      <div class="tr">
        <div class="td">Quantity</div>
        <div class="td"><%=quantity%></div>
      </div>
    </div>
    <% if(resolve == true){%>
      <% if(status == true){ %>
        <div class="tr">
          <div class="td">Delivery</div>
          <div class="td"><%=ExpectedDate%>></div>
        </div>
      <%}else{%>
        <div class="seller-msg seller-msg-rejected">
          <p>Sorry, Order Rejected By Seller your money will be refunded within 2 business days</p>
        </div>
      <%}%>
    <%}else{%>
      <div class="seller-msg seller-msg-pending">
        <p>Status Pending</p>
      </div>
    <%}%>
  </div>
  