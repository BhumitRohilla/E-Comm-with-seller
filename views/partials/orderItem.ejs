<div class="admin-item-container">
    <img src=<%=`/image/product/${item.img}`%> alt="">
    <div class="table">
      <div class="tr">
        <div class="td">Title</div>
        <div class="td"><%=item.title%></div>
      </div>
      <div class="tr">
        <div class="td">Tag</div>
        <div class="td"><%=item.tag.join(' ')%></div>
      </div>
      <div class="tr">
        <div class="td">Date</div>
        <div class="td"><%=item.date%></div>
      </div>
      <div class="tr">
        <div class="td">Status</div>
        <div class="td"><%=item.status%></div>
      </div>
      <div class="tr">
        <div class="td">UserReviews</div>
        <div class="td"><%=item.userReviews%></div>
      </div>
      <div class="tr">
        <div class="td">Quantity</div>
        <div class="td"><%=item.quantity%></div>
      </div>
    <% if(item.resolve!=null){%>
      <div class="tr">
        <div class="td">Delivery</div>
        <div class="td"><%=item.resolve%>></div>
      </div>
    <%}%>
    </div>
  </div>
  